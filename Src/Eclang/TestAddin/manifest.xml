<addin xmlns="EcIDE.Core">

  <meta>
    <author>Chris Anders</author>
    <name>TestAddin</name>
    <version>1.0.0.0</version>
    <icon>nshare_icon.png</icon>
  </meta>

  <extension path="/EcIDE/StartupCommands">
    <command class="{namespace}.Startup" />
  </extension>
  
  <extension path="/EcIDE/Intellisense/Snippets">
    <command class="{namespace}.Intellisense.Snippets.ForSnippet" />
  </extension>
  
  <extension path="/EcIDE/Intellisense/Commands">
    <command class="{namespace}.Intellisense.Commands.KeywordsCommand" />
    <command class="{namespace}.Intellisense.Commands.ClassesCommand" />
    <command class="{namespace}.Intellisense.Commands.AttributeCommand" />
    <command class="{namespace}.Intellisense.Commands.NumberCommand" />
    <command class="{namespace}.Intellisense.Commands.CommentCommand" />
  </extension>

</addin>